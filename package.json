{
  "name": "sim",
  "version": "1.0.0",
  "description": "SynMax Intelligence Marketplace",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "server": "tsx server/server.ts",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,html,css}\""
  },
  "keywords": [
    "marketplace",
    "synmax",
    "maritime"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@reduxjs/toolkit": "^2.8.0",
    "@vitejs/plugin-react": "^4.4.1",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.3",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^22.15.15",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-jsdoc": "^50.6.14",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.7.0",
    "typescript-eslint": "^8.32.0"
  }
}
